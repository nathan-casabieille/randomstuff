# --- Files
alias la="ls -A"
alias l="ls -CF"
alias ll="ls -alF"
alias lq="ls -AlhF"
alias cx="chmod +x"
alias untar="tar -zxvf"
alias ff="find . -type f -iname"
alias mrclean="find -type f \( -name \"*~\" -o -name \"#*#\" -o  -name \"*.o\" \) -delete"

# --- Emacs
alias ne="emacs -nw"

# --- Git
alias gs="git status"
alias ga="git add -A"
alias gc="git checkout"
alias gb="git branch"
